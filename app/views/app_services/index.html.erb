<p id="notice"><%= notice %></p>

<h1>Listing App Services</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_services.each do |app_service| %>
      <tr>
        <td><%= app_service.name %></td>
        <td><%= app_service.description %></td>
        <td><%= link_to 'Show', app_service %></td>
        <td><%= link_to 'Edit', edit_app_service_path(app_service) %></td>
        <td><%= link_to 'Destroy', app_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App service', new_app_service_path %>
