<p id="notice"><%= notice %></p>

<h1>Listing Specific Locations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Location</th>
      <th>Photo</th>
      <th>City</th>
      <th>Area type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @specific_locations.each do |specific_location| %>
      <tr>
        <td><%= specific_location.name %></td>
        <td><%= specific_location.description %></td>
        <td><%= specific_location.location %></td>
        <td><%= specific_location.photo %></td>
        <td><%= specific_location.city %></td>
        <td><%= specific_location.area_type %></td>
        <td><%= link_to 'Show', specific_location %></td>
        <td><%= link_to 'Edit', edit_specific_location_path(specific_location) %></td>
        <td><%= link_to 'Destroy', specific_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Specific location', new_specific_location_path %>
