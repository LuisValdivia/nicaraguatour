<p id="notice"><%= notice %></p>

<h1>Listing Tourist Site App Services</h1>

<table>
  <thead>
    <tr>
      <th>Tourist site</th>
      <th>App service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tourist_site_app_services.each do |tourist_site_app_service| %>
      <tr>
        <td><%= tourist_site_app_service.tourist_site %></td>
        <td><%= tourist_site_app_service.app_service %></td>
        <td><%= link_to 'Show', tourist_site_app_service %></td>
        <td><%= link_to 'Edit', edit_tourist_site_app_service_path(tourist_site_app_service) %></td>
        <td><%= link_to 'Destroy', tourist_site_app_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

